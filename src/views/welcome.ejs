    <!-- Declare a new AngularJS app and associate the controller -->
    <body ng-app ng-controller="reciveOrders">

      <form name="signIpForm"  method="post" action="/auth/signin/">
        <div class="jumbotron">
          <h1 align="center">BREAK</h1>
          <br>
          <div id="todo-form" class="row">
            <div class="col-sm-8 col-sm-offset-2 text-center">
              <div class="form-group">
                <input type="text" name="username" class="form-control input-md text-center" placeholder="Phone number">
                <br>
                <input type="password" name="password" class="form-control input-md text-center" placeholder="Password">
                <br>
                <button type="submit" class="btn btn-success btn-block" >Sign in!</button>
              </div>
              <a button type="submit" class="btn btn-link btn-sm"  href="/Client">Client?</button>
              <a button type="submit" class="btn btn-link btn-sm" href="/Signup">Sign up!</button>

                </div>
              </div>
            </div>
          </form>


